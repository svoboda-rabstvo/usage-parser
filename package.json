{
    "author": "linterhub <hi@linterhub.com>",
    "name": "@linterhub/usage-parser",
    "version": "0.0.0-development",
    "description": "Simple parser for usage page",
    "license": "MIT",
    "repository": "linterhub/usage-parser",
    "bugs": "https://github.com/linterhub/usage-parser/issues",
    "bin": {
        "usage-parser": "bin.js"
    },
    "main": "./dist/index.js",
    "scripts": {
        "jsdoc2md": "./node_modules/.bin/jsdoc2md",
        "ts-node": "./node_modules/mocha/bin/ts-node",
        "mocha": "./node_modules/mocha/bin/mocha",
        "build": "tsc -p .",
        "lint": "tslint -c tslint.json --project . -t codeFrame",
        "test": "nyc mocha -r ts-node/register ./test/*.test.ts",
        "doc": "jsdoc2md --files ./src/**/*.ts --configure ./config/jsdoc2md.json > ./doc/API.md",
        "semantic-release": "semantic-release",
        "log": "npm ls"
    },
    "files": [
        "dist/",
        "README.md"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run test"
        }
    },
    "nyc": {
        "check-coverage": true,
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "coverage/**/*",
            "test/**/*",
            "dist/**/*",
            "src_old/**/*",
            "test_old/**/*",
            "test.1/**/*",
            "spec/**/*",
            "lib/**/*",
            "src/temp/**/*",
            "src/index.ts",
            "**/*.d.ts"
        ],
        "reporter": [
            "html",
            "text-summary"
        ],
        "all": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ]
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "tag": "latest",
        "access": "public"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.40",
        "@babel/core": "^7.0.0-beta.40",
        "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
        "@babel/preset-env": "^7.0.0-beta.40",
        "@babel/preset-typescript": "^7.0.0-beta.40",
        "@semantic-release/changelog": "^3.0.0",
        "@semantic-release/git": "^7.0.1",
        "@types/chai": "^4.1.6",
        "@types/lodash": "^4.14.117",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.11.4",
        "@types/sinon": "^5.0.5",
        "assert": "^1.4.1",
        "chai": "^4.1.2",
        "husky": "^0.14.3",
        "jsdoc-babel": "^0.4.0-alpha.0",
        "jsdoc-to-markdown": "^4.0.1",
        "lodash": "^4.17.11",
        "mocha": "^5.2.0",
        "npx": "^10.2.0",
        "nyc": "^13.1.0",
        "semantic-release": "^15.9.5",
        "sinon": "^7.1.1",
        "ts-node": "^7.0.0",
        "tslint": "^5.11.0",
        "typescript": "^3.1.3",
        "typings": "^2.1.1"
    },
    "dependencies": {
        "commander": "^2.17.1"
    }
}
